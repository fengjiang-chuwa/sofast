[
{
  'repeat(5, 10)':
  {
    id: '{{objectId()}}',
    index: '{{index()}}',
    _name:
    {
      first: '{{firstName()}}',
      last: '{{surname()}}'
    },
    random_name: "{{firstName()}} {{surname()}}",
    name: function(tags)
    {
      return (this._name.first + '.' + this._name.last);
    },
    email: function(tags)
    {
      return (this._name.first + '.' + this._name.last + '@' + this.company + tags.domainZone()).toLowerCase();
    }
  }
}]